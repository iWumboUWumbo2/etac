use io
use conv

multiply(a:int, b:int) : int{
    return a * b;
}

add_multiply (a:int, b:int, c:int):int {
    return multiply(a + b, c);
}

int main() {
    x:int = 2
    y:int = 3
    z:int = 4;
    result:int = add_multiply(x, y, z);

    i:int = 0;
    while (i < 10000000){
        result = result + add_multiply(x,y,z);
        i = i + 1;
    }

    printf("Result: %d\n", result);

    return 0;
}