use conv
use io

main(args:int[][]) {
    N:int = 10;

    i:int;
    j:int;
    k:int;

    a:int[N]
    b:int[N]
    c:int[N]

    i = 0
    while (i<N) {
        a[i] = i;
        b[i] = i + 1;
        i = i+1;
    }

    i = 0
    while (i<N) {
        j = 0
        while (j<N) {
            c[j] = a[j] * b[j];
            j = j+1;
        }

        k = 0
        while (k<N) {
            a[k] = c[k] + 1;
            k = k + 1;
        }
        i = i + 1
    }

    i = 0
    while(i < N) {
        println(unparseInt(a[i]));
        i = i + 1;
    }

}