use io
use conv
x : int = 0
main(args: int[][]) {

	printBool(f1()) // +1
	printBool(f2()) // +1
	printBool(f1()&f2()) // +1

	println(unparseInt(x)) // 3

	x = 0

	if (f1()&f2()){ // false +1
		println("no")
	}
	println(unparseInt(x)) // 1
