use io
use conv
main(args: int[][]) {
	a : int[] = {1,2,3}
	i : int = 0
	res : int[3]
	printBool(length(res)==length(a))
	while(i<length(a)){
		res[i] = inc(a,i)
		i = i + 1
	}
	printArr(a)
	printArr(res)
}

// side effect
inc(a:int[],i:int):int{
	a[i] = a[i] + 1
	return a[i]
}

printBool(b:bool){
  if(b){
    println("true")
  }
  else{
    println("false")
  }
}

printArr(arr:int[]){
	i : int = 0
	while(i<length(arr)){
		print(unparseInt(arr[i])+" ")
		i = i + 1
	}
}