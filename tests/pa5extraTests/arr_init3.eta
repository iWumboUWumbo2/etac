use io
use conv
main(args: int[][]) {
	a : int[][] = {create(1),create(2),create(3)}
	print2dArr(a)
}

create(i:int):int[]{
	a : int[i]
	index : int = 0
	while(index<i){
		a[index] = i
		index = index + 1
	}
	return a
}

print2dArr(a:int[][]){
	i : int = 0
	while(i<length(a)){
		j : int = 0
		while(j<length(a[i])){
			print(unparseInt(a[i][j])+" ")
			j = j + 1
		}
		println("")
		i = i + 1
	}
}