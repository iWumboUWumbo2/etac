use io
use conv
main(args: int[][]) {
	a : int = fac(5)
	b : int = fac_rec(5)
	println(unparseInt(a))
	println(unparseInt(b))
	printBool(a==b)
}

fac(n: int): int {
	i: int = 1
	result: int = 1
	while (i <= n) {
		result = result * i
		i=i+1
	}
	return result
}

fac_rec(n:int):int{
	if (n<=1){
		return 1
	}
	else{
		return n * fac_rec(n-1)
	}
}

printBool(b:bool){
  if(b){
    println("true")
  }
  else{
    println("false")
  }
}