use io
use conv
main(args: int[][]) {
	printBool(false | false & true) //false
	printBool((false | false) & true) //false
	printBool((false | true) & true) //true
  printBool(false | true & false) //false

  printBool(true | true & false) // true
  printBool((true | true) & false) // false
}

printBool(b:bool){
  if(b){
    println("true")
  }
  else{
    println("false")
  }
}
