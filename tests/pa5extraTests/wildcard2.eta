use io
use conv
main(args: int[][]) {
	a: int, _, c: int[], _, _ = foo()
	println(unparseInt(a))
	printArr(c)
}

foo():int,bool,int[],int[],int[][]{
	return 1,true,{1,3},{},{{1},{2,2}}
}

printBool(b:bool){
  if(b){
    println("true")
  }
  else{
    println("false")
  }
}

printArr(arr:int[]){
	i : int = 0
	while(i<length(arr)){
		print(unparseInt(arr[i])+" ")
		i = i + 1
	}
	println("")
}

print2dArr(a:int[][]){
	i : int = 0
	while(i<length(a)){
		j : int = 0
		while(j<length(a[i])){
			print(unparseInt(a[i][j])+" ")
			j = j + 1
		}
		println("")
		i = i + 1
	}
}