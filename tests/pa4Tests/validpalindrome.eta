isPalindrome(s: int[]): bool {
    l : int =  0
    r : int = length(s) - 1

    while (l < r) {
        if (s[l] != s[r]) {
            return false;
        }

        l = l + 1
        r = r - 1;
    }
    return true
}

main(args: int[][]){
    a : int[] = "abababababababa"; // true
    b: int[]= "amanaplanacanalpanama"; // true
    c : int[] = " " //true
    d : int[] = "raceacar" // false

    e : bool = isPalindrome(a);
    f : bool = isPalindrome(b);
    g : bool = isPalindrome(c);
    h : bool = isPalindrome(d);
}