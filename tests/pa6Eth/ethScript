#build

etac ("Test --help") "--help" {

}

 etac ("Test --irgen") "-libpath $(testpath) --irgen" {

 }

 etac ("Test --irgen -O") "-libpath $(testpath) --irgen -O" {

     chainedExprs.eta;
     constprop.eta;
     copyprop.eta;
     dce1.eta;
     dce2.eta;
     dce3.eta;
     recmain.eta;

 }
etac ("Test -target linux") "-libpath $(testpath) -target linux" {


   swap.eta;
    chainedExprs.eta;
    constprop.eta;
    copyprop.eta;
    dce1.eta;
    dce2.eta;
    dce3.eta;
    recmain.eta;

}

etac ("Test -target linux -O") "-libpath $(testpath) -target linux -O" {

    swap.eta;

    chainedExprs.eta;
    constprop.eta;
    copyprop.eta;
    dce1.eta;
    dce2.eta;
    dce3.eta;
    recmain.eta;

}
