use io
use conv


main(args:int[][]) {
    x:int = 4000000000
    y:int = 5000000000

    a:int = x + 10;
    b:int = y * 2;
    c:int = a + b;
    d:int = c / 3;

    if (d >= 10 & d <= 20) {
        while (d > 0) {
            //println(unparseInt(d))
            d = d - 1;
        }
    } else {
        i:int = 0;
        while (i < d) {
            //println(unparseInt(i))
            i = i+1;
        }
    }

    println("Done");
}