use io
use conv

main(args : int[][]) {
    a : int = 5;
    while a < 100 {
        a = a + 5;
        if a > 51 {
            a = 1000;
            if a == 1000 {
                a = 2000;
                break;
            }
        }
    }
    println(unparseInt(a)); // 2000

    b : int = 5
    while a > 1500 {
        a = a - b;
        while a > 1900 {
            b = b + 5
            if b >= 100 {
                break;
            }
        }
    }

    println(unparseInt(a)); //1495
}