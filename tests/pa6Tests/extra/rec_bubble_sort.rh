use multi_intf
use io
use conv

main(args: int[][]){
    arr: point[] = {Point(1,7), Point(4,5), Point(2,8), Point(8,2), Point(5,4), Point(7,1)}
    n: int = 6
    i: int = 0
    while(i < n - 1){
        j: int = 0
        while(j < n - i - 1){
            if(arr[j].x > arr[j + 1].x) arr[j], arr[j+1] = arr[j+1], arr[j]
            j = j + 1
        }
        i = i + 1
    }
    i = 0
    while(i < n){
        print(unparseInt(arr[i]).x)
        print(unparseInt(arr[i]).y)
        i = i + 1
    }
}