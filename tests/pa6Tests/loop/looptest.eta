use io
use conv

main(args : int[][]) {
    i:int = 0;
    println(unparseInt(foo(fact(3),fact(1),fact(4),fact(4))))
}

foo(a:int,b:int,c:int,d:int):int{
    res:int  = 5;
    i:int = 0;
    while (i < 6234121100){
        y:int = i * 5;
        res = res + y;
        i = i + 1;
    }
    return res;
}

fact(n:int):int{
    if (n <= 1){
        return 1;
    }
    return n * fact(n-1);
}

