use io
use conv

main(args: int[][]) {
	arr: int[10]
	// plus5(arr,1) = 1, arr[1] -> 5, arr[5] = arr[]
	// set arr[5] to 1005
	arr[arr[plus5(arr,1)]] = arr[arr[plus5(arr,1)]]+1000
	printArr(arr)
	// set arr[5] to 2010
	arr[arr[plus5(arr,1)]] = arr[arr[plus5(arr,1)]]+1000
}

plus5(arr:int[],i:int):int{
	arr[i] = arr[i] + 5
	return i
}

printArr(arr:int[]){
	i : int = 0
	while(i < length(arr)) {
		print(unparseInt(arr[i]) + " ")
		i = i + 1
	}
}